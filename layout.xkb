xkb_keymap {

	xkb_keycodes  {
    include "evdev+aliases(qwerty)"
    <RET> = 255;
  };

	xkb_types     { include "complete"	};

	xkb_compat    { include "complete"	};

	xkb_symbols   {
    include "pc+us(dvp)+ctrl(nocaps)+keypad(atm)"
    key.type = "ONE_LEVEL";
    key <RTRN> { [ Control_R ] };
    key <RET> { [ Return ] };
  };

};
