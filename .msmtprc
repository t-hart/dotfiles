auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.msmtp.log

# Gmail
account        gmail
host           smtp.gmail.com
port           587
from           thomasheartman@gmail.com
user           thomasheartman
passwordeval   fish -c "bw get password gheart"


# Set a default account
account default : gmail
